@model List<WishList.Models.Item>

<html xmlns="http://www.w3.org/1999/xhtml">
    <h1>Wishlist</h1>
    <a asp-action="create">Add Item</a>
    <ul>
        @foreach (var item in Model)
    {
        <li>@item.Description</li>
        <a asp-action=""delete" asp-route-id="Id">delete</a>
    }
    </ul>
</html>